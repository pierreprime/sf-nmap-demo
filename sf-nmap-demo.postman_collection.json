				"body": {
					"mode": "raw",
					"raw": "{\n\t\"ipRange\": [\n\t\t\"{{network_v4}}\"\n\t],\n\t\"command\": \"discover_ips\",\n\t\"flags\": [\n\t\t\"-sP\"\n\t],\n\t\"verbose\": 1,\n\t\"ports\": []\n}"
